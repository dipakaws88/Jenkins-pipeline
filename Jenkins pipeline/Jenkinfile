pipeline {
    agent any
    stages {
        stage (BUILD)
        {
            steps {
            echo "This is build step"
                }
        }
        stage (Test)
        {
            steps {
                echo "This is test step"
                  }
        }
        stage (deploy)
        {
            steps {
              echo "This is deploy step"
                    }
                }
    }
}
